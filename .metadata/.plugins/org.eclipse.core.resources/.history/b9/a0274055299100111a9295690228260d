
public class MyRunnable implements Runnable{

	@Override
	public void run() {
		
	}
	
	void Test()
	{
		System.out.println("Thread entered: " + Thread.currentThread().getId());
		for (int i = 0; i < 10; i++) {
			Thread.sleep(100);
			System.out.println("Thread: " + Thread.currentThread().getId() + "; " + i);
		}
	}

	
}
