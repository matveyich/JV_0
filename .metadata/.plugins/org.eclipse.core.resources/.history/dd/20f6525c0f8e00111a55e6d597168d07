
public class FirstClass {
	
	public static int i1 = 5;
	public int i2 = 5;
	
	public void printOneWord(){
		System.out.println("Print from non static method");
	}
	
	public static void printSecondWord(){
		System.out.println("Print from static method");
	}
	
	public int add(int x, int y){
		return x+y;
	}
	
}
