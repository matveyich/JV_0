
public class Demo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int dayNumber = 11;
		switch (dayNumber){
			case 1:
			case 2:
			case 3:
			case 4:
			case 5:
				System.out.println("Work day");
				break;
			case 6:
			case 7:
				System.out.println("Holiday");
				break;
			default:
				System.out.println("Wrong day number");
				break;
		}
		
	}

}
