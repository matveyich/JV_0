
public class ItWorker {
	public int id;
	public int age;
	public String name;
	public String surname;
	public String salary
	
	public ItWorker(int id, int age, String name, String surname, String salary){
		this.id = id;
		this.age = age;
		this.name = name;
		this.surname = surname;
		this.salary = salary;
	}
}
